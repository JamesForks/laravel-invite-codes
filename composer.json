{
    "name": "mateusjunges/laravel-invites",
    "description": "Limit access to private resources by using laravel invites",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mateus Junges",
            "email": "contato@mateusjunges.com",
            "homepage": "https://twitter.com/mateusjungess",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/auth": "^5.6|^5.7|^5.8|^6.0",
        "illuminate/support": "^5.6|^5.7|^5.8|^6.0",
        "illuminate/database": "^5.6|^5.7|^5.8|^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.6|^3.7|^3.8",
        "phpunit/phpunit": "^7.0|^8.0",
        "predis/predis": "^1.1",
        "facade/ignition": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Junges\\Watchdog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Junges\\Watchdog\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Junges\\Invites\\InviteServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests"
    }
}
